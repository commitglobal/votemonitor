{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "None"
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        }
    },
    "AllowedHosts": "*",
    "AuthFeatureConfig": {
        "JWTConfig": {
            "TokenSigningKey": "SecretKeyOfDoomThatMustBeAMinimumNumberOfBytes",
            "TokenExpirationInMinutes": 10080,
            "RefreshTokenExpirationInDays": 30
        }
    },
    "PollingStationsFeatureConfig": {
        "ParserConfig": {
            "MaxParserErrorsReturned": 100
        }
    },
    "Domain": {
        "DbConnectionConfig": {
            "Server": "localhost",
            "Port": "5432",
            "Database": "vote-monitor",
            "UserId": "postgres",
            "Password": "docker"
        }
    },
    "Core": {
        "EnableHangfire":  true,
        "HangfireConnectionConfig": {
            "Server": "localhost",
            "Port": "5432",
            "Database": "vote-monitor-hangfire",
            "UserId": "postgres",
            "Password": "docker"
        }
    },
    "FileStorage": {
        "FileStorageType": "LocalDisk",
        "LocalDisk": {
            "Path": "Uploads"
        },
        "S3": {
            "BucketName": "",
            "PresignedUrlValidityInSeconds": 3600,
            "AWSRegion": "eu-central-1",
            "AWSAccessKey": "youraccesskey",
            "AWSSecretKey": "yoursecretkey"
        }
    },
    "PushNotifications": {
        "SenderType": "Noop",
        "Firebase": {
            "Token": "TBD",
            "BatchSize": 100
        }
    },
    "Sentry": {
        "Enabled": true,
        "Dsn": "",
        "TracesSampleRate": 0.2
    },
    "ApiConfiguration": {
        "WebAppUrl": "http://localhost:5173"
    },
    "Mailing": {
        "MailSenderType": "SMTP",
        "SMTP": {
            "SenderName": "John Doe",
            "SenderEmail": "john@example.com",
            "Host": "smtp.example.com",
            "Port": 587,
            "UserName": "user@example.com",
            "Password": "password123"
        },
        "SES": {
            "SenderName": "John Doe",
            "SenderEmail": "john@example.com",
            "AWSRegion": "eu-central-1",
            "AWSAccessKey": "youraccesskey",
            "AWSSecretKey": "yoursecretkey"
        }
    },
    "Seeders": {
        "PlatformAdminSeeder": {
        }
    }
}
