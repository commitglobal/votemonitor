{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "None"
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        }
    },
    "AllowedHosts": "*",
    "AuthFeatureConfig": {
        "JWTConfig": {
            "TokenSigningKey": "SecretKeyOfDoomThatMustBeAMinimumNumberOfBytes"
        }
    },
    "PollingStationsFeatureConfig": {
        "ParserConfig": {
            "MaxParserErrorsReturned": 100
        }
    },
    "Domain": {
        "DbConnectionConfig": {
            "Server": "localhost",
            "Port": "5432",
            "Database": "vote-monitor",
            "UserId": "postgres",
            "Password": "docker"
        }
    },
    "FileStorage": {
        "FileStorageType": "LocalDisk",
        "LocalDisk": {
            "Path": "Uploads"
        },
        "S3": {
            "BucketName": "",
            "PresignedUrlValidityInSeconds": 3600,
            "AWSRegion": "eu-central-1",
            "AWSAccessKey": "youraccesskey",
            "AWSSecretKey": "yoursecretkey"
        }
    },
    "PushNotifications": {
        "SenderType": "Noop",
        "Firebase": {
            "Token": "TBD",
            "BatchSize": 100
        }
    },
    "Sentry": {
        "Enabled": true,
        "Dsn": "",
        "TracesSampleRate": 0.2
    },
    "AWS": {
        "Region": "eu-central-1",
        "AWS_ACCESS_KEY_ID": "key",
        "AWS_SECRET_ACCESS_KEY": "key"
    }
}
