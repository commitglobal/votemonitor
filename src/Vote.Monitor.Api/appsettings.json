{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "None"
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        }
    },
    "AllowedHosts": "*",
    "AuthFeatureConfig": {
        "JWTConfig": {
            "TokenSigningKey": "SecretKeyOfDoomThatMustBeAMinimumNumberOfBytes"
        }
    },
    "PollingStationsFeatureConfig": {
        "ParserConfig": {
            "MaxParserErrorsReturned": 100
        }
    },
    "Domain": {
        "DbConnectionConfig": {
            "Server": "localhost",
            "Port": "5432",
            "Database": "vote-monitor",
            "UserId": "postgres",
            "Password": "docker"
        }
    },
    "FileStorage": {
        "FileStorageType": "LocalDisk",
        "LocalDisk": {
            "Path": "Uploads"
        },
        "S3": {
            "TBD": "TBD"
        }
    },
    "PushNotifications": {
        "SenderType": "Noop",
        "Firebase": {
            "Token": "TBD",
            "BatchSize": 100
        }
    }
}
