{
    "profiles": {
        "Vote.Monitor.Api": {
            "commandName": "Project",
            "launchBrowser": true,
            "launchUrl": "swagger",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "applicationUrl": "https://localhost:7123;http://localhost:5043"
        },
        "Docker": {
            "commandName": "Docker",
            "launchBrowser": true,
            "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
            "environmentVariables": {
                "ASPNETCORE_URLS": "https://+:443;http://+:80"
            },
            "publishAllPorts": true,
            "useSSL": true
        }
    }
}