{
  "expo": {
    "name": "VoteMonitor",
    "slug": "vote-monitor",
    "version": "2.0.4",
    "scheme": "mobile",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDD20C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.commitglobal.votemonitor",
      "buildNumber": "71",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "versionCode": 71,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#FDD20C"
      },
      "package": "org.commitglobal.votemonitor.app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "93e17628-ddcd-4e67-849c-b356a18a6082"
      },
      "message": "Fix set completion on PSI",
      "updateCritical": 48,
      "updateVersion": 56
    },
    "owner": "commit-global",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/93e17628-ddcd-4e67-849c-b356a18a6082"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/DMSans-Bold.ttf",
            "./assets/fonts/DMSans-Medium.ttf",
            "./assets/fonts/DMSans-Regular.ttf",
            "./assets/fonts/Roboto-Bold.ttf",
            "./assets/fonts/Roboto-Medium.ttf",
            "./assets/fonts/Roboto-Regular.ttf"
          ]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/icon.png",
          "color": "#FDD20C"
        }
      ],
      [
        "@morrowdigital/watermelondb-expo-plugin"
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.6.10",
            "packagingOptions": {
              "pickFirst": [
                "**/libc++_shared.so"
              ]
            }
          }
        }
      ],
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to your photos to upload images or videos to attach in support of your observation notes or quick reports.",
          "cameraPermission": "Allow access to your camera to take photos or videos to attach in support of your observation notes or quick reports."
        }
      ],
      "expo-secure-store",
      [
        "@sentry/react-native/expo",
        {
          "organization": "code4romania",
          "project": "votemonitor-mobile"
        }
      ],
      "react-native-compressor"
    ]
  }
}