{
  "expo": {
    "name": "VoteMonitor",
    "slug": "vote-monitor",
    "version": "0.0.36",
    "scheme": "mobile",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5F288D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.commitglobal.votemonitor",
      "buildNumber": "36",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "versionCode": 36,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#5F288D"
      },
      "package": "org.commitglobal.votemonitor"
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "93e17628-ddcd-4e67-849c-b356a18a6082"
      },
      "message": "Client generated ids",
      "updateCritical": 1,
      "updateVersion": 1
    },
    "owner": "commit-global",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/93e17628-ddcd-4e67-849c-b356a18a6082"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/DMSans-Bold.ttf",
            "./assets/fonts/DMSans-Medium.ttf",
            "./assets/fonts/DMSans-Regular.ttf",
            "./assets/fonts/Roboto-Bold.ttf",
            "./assets/fonts/Roboto-Medium.ttf",
            "./assets/fonts/Roboto-Regular.ttf"
          ]
        }
      ],
      [
        "@morrowdigital/watermelondb-expo-plugin"
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.6.10",
            "packagingOptions": {
              "pickFirst": [
                "**/libc++_shared.so"
              ]
            }
          }
        }
      ],
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload them.",
          "cameraPermission": "The app accesses your camera to let you take photos or videos and upload them."
        }
      ],
      "expo-secure-store",
      [
        "@sentry/react-native/expo",
        {
          "organization": "code4romania",
          "project": "votemonitor-mobile"
        }
      ]
    ]
  }
}