{
  "cli": {
    "version": ">= 3.13.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "android-apk": {
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "testing" // https://github.com/expo/eas-cli#eas-channeledit-name
    },
    "production": {
      "channel": "production"
    },
    "staging": {
      "channel": "staging"
    }
  }
}
